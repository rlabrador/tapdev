<openerp>
    <data>
        <template id="index">
            <title>Tapmain</title>
            <t t-foreach="usertests" t-as="usertest">
              <p><t t-esc="usertest.id"/><t t-esc="usertest.name"/></p>
            </t>
        </template>


         <template id="object">
           <h1><t t-esc="object.display_name"/></h1>
           <dl>
             <t t-foreach="object._fields" t-as="field">
               <dt><t t-esc="field"/></dt>
               <dd><t t-esc="object[field]"/></dd>
             </t>
           </dl>
         </template>
    </data>
</openerp>